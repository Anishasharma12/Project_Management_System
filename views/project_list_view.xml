<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="wb_project_employee_master_tree_view" model="ir.ui.view">
        <field name="name">wb.project_listview.tree.view</field>
        <field name="model">wb.project_listview</field>
        <field name="arch" type="xml">
            <tree>
                <!-- <field name="project_id" />  -->
                <button name="%(project_list_per_month_id)d" string="summery" type="action" class="btn-primary"
                context="{'default_project_id': active_id}"/>
                <field name="name" /> 
                <field name="code" />     
                <field name="op_planned_hours" />    
                <field name="op_actual_hours" />
                <field name="planned_cost" />
                <field name="actual_cost" />
            </tree>
        </field>
    </record>
    <record id="wb_project_employee_master_form_view" model="ir.ui.view">
        <field name="name">wb.project_listview.tree.view</field>
        <field name="model">wb.project_listview</field>
        <field name="arch" type="xml">
            <form string="Project">
            <sheet>
                <group>
                    <field name="project_id" />
                    <field name="name" />  
                    <field name="code" /> 
                </group>
            </sheet>
            </form>
        </field>
    </record>
</odoo>
