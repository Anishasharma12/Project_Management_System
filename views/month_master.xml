<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="month_id_2" model="ir.actions.act_window">
            <field name="name">Open Another Model Form</field>
            <field name="type">ir.actions.act_window</field>
            <field name="target">current</field>
            <!-- <field name="view_id" ref="Project_Management.view_month_master_tree"/> -->
            <field name="res_model">month.master</field>
            <field name="view_mode">tree</field>
        </record>

  <record id="view_month_master_form" model="ir.ui.view">
    <field name="name">month.master.form</field>
    <field name="model">month.master</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <field name="month"/>
            <field name="order"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>

  <record id="view_month_master_tree" model="ir.ui.view">
    <field name="name">month.master.tree</field>
    <field name="model">month.master</field>
    <field name="arch" type="xml">
      <tree>
        <field name="id"/>
        <field name="month"/>
        <field name="order"/>
      </tree>
    </field>
  </record>
</odoo>
